{
    "name": "phpprofiler/profiler",
    "description": "PHP Proflier",
    "type": "library",
    "keywords": ["php", "profiler"],
    "homepage": "https://the-php-profiler.github.io/",
    "license": "OSL-3.0",
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "ext-openssl": "*"
    },
    "suggest": {
        "php-64bit": ">=8.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "John Leveradge",
            "email": "johnleveradge@gmail.com",
            "homepage": "https://github.com/JohnLeveradge",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "johnleveradge@gmail.com",
        "issues": "https://github.com/The-PHP-Profiler/Profiler/issues",
        "forum": "https://github.com/The-PHP-Profiler/Profiler/discussions",
        "wiki": "https://github.com/The-PHP-Profiler/Profiler/wiki",
        "source": "https://github.com/The-PHP-Profiler/Profiler",
        "docs": "https://the-php-profiler.github.io/"
    },
    "config": {
        "process-timeout": 300,
        "preferred-install": {
            "*": "dist"
        },
        "disable-tls": false,
        "secure-http": true,
        "vendor-dir": "vendor",
        "bin-dir": "vendor/bin",
        "prepend-autoloader": false,
        "sort-packages": true
    }
}
